{
  "version": 2,
  "builds": [
    {
      "src": "vivah/asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vivah/asgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "vivah.settings"
  },
  "buildCommand": "python manage.py collectstatic --noinput",
  "outputDirectory": "staticfiles"
}
