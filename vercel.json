{
  "version": 2,
  "builds": [
    { "src": "vivah/asgi.py", "use": "@vercel/python" },
    { "src": "manage.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "vivah/asgi.py" }
  ],
  "buildCommand": "python manage.py collectstatic --noinput",
  "outputDirectory": "staticfiles"
}
